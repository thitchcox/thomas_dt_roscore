<launch>
	<arg name="veh"/>
	<arg name="param_file_name" default="default" doc="Specify a param file. ex:megaman"/>
	<arg name="param_file_name_wheels_driver_node" default="default" doc="Specify a param file. ex:megaman"/>
	<arg name="pkg_name" default="simple_estimator_pkg" doc="name of the package"/>
	<arg name="node_name" default="simple_estimator_node" doc="name of the node"/>

        <group ns="$(arg veh)">
	    <node  pkg="$(arg pkg_name)" type="$(arg node_name).py" name="$(arg node_name)" output="screen" required="true">
	        <!-- <rosparam command="load" file="$(find custom_control)/config/$(arg node_name)/$(arg param_file_name).yaml"/> -->
	    </node>
	</group>

</launch>
