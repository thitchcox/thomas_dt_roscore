<launch>
    <arg name="veh" doc="Name of vehicle. ex: megaman"/>
    <arg name="param_file_name" default="default" doc="Specify a param file. ex:megaman." />

    <arg name="pkg_name" value="dt_project_lane_point_filter"/>
    <arg name="node_name" default="dt_project_lane_point_filter_node"/>

    <arg name="verbose" default="false" />

    <group ns="$(arg veh)">
        <node  name="dt_project_lane_point_filter" pkg="$(arg pkg_name)" type="$(arg node_name).py" output="screen" required="true">
            <rosparam command="load" file="$(find dt_project_lane_point_filter)/config/$(arg node_name)/$(arg param_file_name).yaml"/>
            <param name="verbose" value="$(arg verbose)"/>
        </node>
    </group>
</launch>

